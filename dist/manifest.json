{   
    "manifest_version": 2, 
    "name": "Browser series list",
    "description": "Simple series list keeper with links and episode counter",
    "version": "1.0",
    "author": "Illia Filatov",
    "browser_action": {
        "default_icon": "./img/icon16.png",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
      },
    "icons": {
        "128": "./img/icon128.png",
        "16": "./img/icon16.png",
        "48": "./img/icon48.png"
        },
    "permissions": [ "storage", "tabs" ]
}